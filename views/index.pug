extends layout

block content
  h1= Webapps
  p Welcome to #{title}
  p This is webapps assignment 10 of Partha Sarathy Boda.
  button(type="button", class="btn btn-secondary", onclick="goFaster()") Press Me
  div
  button#speedButton(type="button", onclick="goFaster()") Increase Speed
  div#faster Increase the speed

  script. 
   var mph = 20;
   var change = 1;
   
   function goFaster() {
     mph += change;
     change += 1;
     document.getElementById("faster").innerText = `MPH ${mph} with change ${change}`;
    }
  a(href="/jackets") search results

